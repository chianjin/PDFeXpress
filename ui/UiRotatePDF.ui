<?xml version='1.0' encoding='utf-8'?>
<interface version="1.1">
    <object class="ttk.Frame" id="UiRotatePDF">
        <property name="height">200</property>
        <property name="width">200</property>
        <layout manager="pack">
            <property name="fill">x</property>
            <property name="propagate">True</property>
            <property name="side">top</property>
        </layout>
        <child>
            <object class="ttk.Frame" id="FrameTitle">
                <property name="height">200</property>
                <property name="padding">10</property>
                <property name="width">200</property>
                <layout manager="pack">
                    <property name="fill">x</property>
                    <property name="propagate">True</property>
                    <property name="side">top</property>
                </layout>
                <child>
                    <object class="ttk.Label" id="LabelFrameName">
                        <property name="font">{Microsoft YaHei UI} 16 {bold}</property>
                        <property name="text" translatable="yes">旋转 PDF</property>
                        <property name="textvariable">string:frame_name</property>
                        <layout manager="pack">
                            <property name="propagate">True</property>
                            <property name="side">left</property>
                        </layout>
                    </object>
                </child>
            </object>
        </child>
        <child>
            <object class="ttk.Labelframe" id="FramePDFFile">
                <property name="height">200</property>
                <property name="text" translatable="yes">PDF 文件</property>
                <property name="width">200</property>
                <layout manager="pack">
                    <property name="fill">x</property>
                    <property name="padx">4</property>
                    <property name="pady">4</property>
                    <property name="propagate">True</property>
                    <property name="side">top</property>
                </layout>
                <child>
                    <object class="ttk.Entry" id="EntryPDFFile">
                        <property name="state">readonly</property>
                        <property name="textvariable">string:pdf_file</property>
                        <layout manager="pack">
                            <property name="expand">true</property>
                            <property name="fill">x</property>
                            <property name="padx">4</property>
                            <property name="pady">4</property>
                            <property name="propagate">True</property>
                            <property name="side">left</property>
                        </layout>
                    </object>
                </child>
                <child>
                    <object class="ttk.Button" id="ButtonPDFFile">
                        <property name="command" type="command" cbtype="simple">get_pdf_file</property>
                        <property name="text" translatable="yes">浏览</property>
                        <layout manager="pack">
                            <property name="padx">4</property>
                            <property name="pady">4</property>
                            <property name="propagate">True</property>
                            <property name="side">right</property>
                        </layout>
                    </object>
                </child>
            </object>
        </child>
        <child>
            <object class="ttk.Labelframe" id="FrameRotatedPDFFile">
                <property name="height">200</property>
                <property name="text" translatable="yes">输出 PDF 文件</property>
                <property name="width">200</property>
                <layout manager="pack">
                    <property name="fill">x</property>
                    <property name="padx">4</property>
                    <property name="pady">4</property>
                    <property name="propagate">True</property>
                    <property name="side">top</property>
                </layout>
                <child>
                    <object class="ttk.Entry" id="EntryRotatedPDFFile">
                        <property name="textvariable">string:rotated_pdf_file</property>
                        <layout manager="pack">
                            <property name="expand">true</property>
                            <property name="fill">x</property>
                            <property name="padx">4</property>
                            <property name="pady">4</property>
                            <property name="propagate">True</property>
                            <property name="side">left</property>
                        </layout>
                    </object>
                </child>
                <child>
                    <object class="ttk.Button" id="ButtonRotatedPDFFile">
                        <property name="command" type="command" cbtype="simple">set_rotated_pdf_file</property>
                        <property name="text" translatable="yes">浏览</property>
                        <layout manager="pack">
                            <property name="padx">4</property>
                            <property name="pady">4</property>
                            <property name="propagate">True</property>
                            <property name="side">left</property>
                        </layout>
                    </object>
                </child>
            </object>
        </child>
        <child>
            <object class="ttk.Labelframe" id="FrameOptions">
                <property name="height">200</property>
                <property name="text" translatable="yes">选项</property>
                <property name="width">200</property>
                <layout manager="pack">
                    <property name="fill">x</property>
                    <property name="padx">4</property>
                    <property name="pady">4</property>
                    <property name="propagate">True</property>
                    <property name="side">top</property>
                </layout>
                <child>
                    <object class="ttk.Label" id="LabelDegree">
                        <property name="text" translatable="yes">旋转角度</property>
                        <layout manager="pack">
                            <property name="padx">4</property>
                            <property name="pady">4</property>
                            <property name="propagate">True</property>
                            <property name="side">left</property>
                        </layout>
                    </object>
                </child>
                <child>
                    <object class="ttk.Combobox" id="ComboboxDegree">
                        <property name="state">readonly</property>
                        <property name="textvariable">string:rotate_degree</property>
                        <property name="values">90° 180° 270°</property>
                        <property name="width">4</property>
                        <layout manager="pack">
                            <property name="padx">4</property>
                            <property name="pady">4</property>
                            <property name="propagate">True</property>
                            <property name="side">left</property>
                        </layout>
                    </object>
                </child>
                <child>
                    <object class="ttk.Frame" id="FrameSpacer">
                        <property name="height">1</property>
                        <property name="width">20</property>
                        <layout manager="pack">
                            <property name="padx">4</property>
                            <property name="pady">4</property>
                            <property name="propagate">True</property>
                            <property name="side">left</property>
                        </layout>
                    </object>
                </child>
                <child>
                    <object class="ttk.Radiobutton" id="RadiobuttonCW">
                        <property name="text" translatable="yes">顺时针</property>
                        <property name="value">cw</property>
                        <property name="variable">string:rotate_direction</property>
                        <layout manager="pack">
                            <property name="padx">4</property>
                            <property name="pady">4</property>
                            <property name="propagate">True</property>
                            <property name="side">left</property>
                        </layout>
                    </object>
                </child>
                <child>
                    <object class="ttk.Radiobutton" id="RadiobuttonCCW">
                        <property name="text" translatable="yes">逆时针</property>
                        <property name="value">ccw</property>
                        <property name="variable">string:rotate_direction</property>
                        <layout manager="pack">
                            <property name="padx">4</property>
                            <property name="pady">4</property>
                            <property name="propagate">True</property>
                            <property name="side">left</property>
                        </layout>
                    </object>
                </child>
                <child>
                    <object class="ttk.Frame" id="FrameSpacer2">
                        <property name="height">1</property>
                        <property name="width">20</property>
                        <layout manager="pack">
                            <property name="padx">4</property>
                            <property name="pady">4</property>
                            <property name="propagate">True</property>
                            <property name="side">left</property>
                        </layout>
                    </object>
                </child>
                <child>
                    <object class="ttk.Checkbutton" id="Checkbutton1">
                        <property name="command" type="command" cbtype="simple">set_use_src_dir</property>
                        <property name="offvalue">0</property>
                        <property name="onvalue">1</property>
                        <property name="text" translatable="yes">输出到原目录</property>
                        <property name="variable">string:use_src_dir</property>
                        <layout manager="pack">
                            <property name="padx">4</property>
                            <property name="pady">4</property>
                            <property name="propagate">True</property>
                            <property name="side">left</property>
                        </layout>
                    </object>
                </child>
            </object>
        </child>
        <child>
            <object class="ttk.Labelframe" id="FrameProcess">
                <property name="height">200</property>
                <property name="text" translatable="yes">旋转 PDF</property>
                <property name="width">200</property>
                <layout manager="pack">
                    <property name="fill">x</property>
                    <property name="padx">4</property>
                    <property name="pady">4</property>
                    <property name="propagate">True</property>
                    <property name="side">top</property>
                </layout>
                <child>
                    <object class="ttk.Progressbar" id="Progressbar">
                        <property name="orient">horizontal</property>
                        <property name="variable">int:progress</property>
                        <layout manager="pack">
                            <property name="fill">x</property>
                            <property name="padx">4</property>
                            <property name="pady">4</property>
                            <property name="propagate">True</property>
                            <property name="side">top</property>
                        </layout>
                    </object>
                </child>
                <child>
                    <object class="ttk.Label" id="LabelPDFInfo">
                        <property name="textvariable">string:pdf_info</property>
                        <layout manager="pack">
                            <property name="padx">4</property>
                            <property name="pady">4</property>
                            <property name="propagate">True</property>
                            <property name="side">left</property>
                        </layout>
                    </object>
                </child>
                <child>
                    <object class="ttk.Label" id="LabelProcessInfo">
                        <property name="textvariable">string:process_info</property>
                        <layout manager="pack">
                            <property name="expand">true</property>
                            <property name="fill">x</property>
                            <property name="padx">4</property>
                            <property name="pady">4</property>
                            <property name="propagate">True</property>
                            <property name="side">left</property>
                        </layout>
                    </object>
                </child>
                <child>
                    <object class="ttk.Button" id="ButtonProcess">
                        <property name="command" type="command" cbtype="simple">process</property>
                        <property name="state">disabled</property>
                        <property name="text" translatable="yes">旋转</property>
                        <layout manager="pack">
                            <property name="padx">4</property>
                            <property name="pady">4</property>
                            <property name="propagate">True</property>
                            <property name="side">left</property>
                        </layout>
                    </object>
                </child>
            </object>
        </child>
    </object>
</interface>
