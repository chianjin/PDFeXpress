<?xml version='1.0' encoding='utf-8'?>
<interface version="1.1">
  <object class="ttk.Frame" id="UiMainFrame">
    <property name="height">200</property>
    <property name="width">200</property>
    <layout manager="pack">
      <property name="propagate">True</property>
      <property name="side">top</property>
    </layout>
    <child>
      <object class="ttk.Labelframe" id="FrameOperateButtons">
        <property name="height">200</property>
        <property name="text" translatable="yes">Operate</property>
        <property name="width">200</property>
        <layout manager="pack">
          <property name="fill">y</property>
          <property name="padx">4</property>
          <property name="pady">4</property>
          <property name="propagate">True</property>
          <property name="side">left</property>
        </layout>
        <child>
          <object class="ttk.Button" id="ButtonMergePDF">
            <property name="command" type="command" cbtype="with_wid">set_operate</property>
            <property name="text" translatable="yes">Merge PDF</property>
            <layout manager="pack">
              <property name="fill">x</property>
              <property name="ipadx">2</property>
              <property name="padx">4</property>
              <property name="pady">4</property>
              <property name="propagate">True</property>
              <property name="side">top</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Button" id="ButtonSplitPDF">
            <property name="command" type="command" cbtype="with_wid">set_operate</property>
            <property name="text" translatable="yes">Split PDF</property>
            <layout manager="pack">
              <property name="fill">x</property>
              <property name="ipadx">2</property>
              <property name="padx">4</property>
              <property name="pady">4</property>
              <property name="propagate">True</property>
              <property name="side">top</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Button" id="ButtonRotatePDF">
            <property name="command" type="command" cbtype="with_wid">set_operate</property>
            <property name="text" translatable="yes">Rotate PDF</property>
            <layout manager="pack">
              <property name="fill">x</property>
              <property name="ipadx">2</property>
              <property name="padx">4</property>
              <property name="pady">4</property>
              <property name="propagate">True</property>
              <property name="side">top</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Button" id="ButtonCompressPDF">
            <property name="command" type="command" cbtype="with_wid">set_operate</property>
            <property name="text" translatable="yes">Compress PDF</property>
            <layout manager="pack">
              <property name="fill">x</property>
              <property name="ipadx">2</property>
              <property name="padx">4</property>
              <property name="pady">4</property>
              <property name="propagate">True</property>
              <property name="side">top</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Button" id="ButtonExtractImages">
            <property name="command" type="command" cbtype="with_wid">set_operate</property>
            <property name="text" translatable="yes">Extract Images</property>
            <layout manager="pack">
              <property name="fill">x</property>
              <property name="ipadx">2</property>
              <property name="padx">4</property>
              <property name="pady">4</property>
              <property name="propagate">True</property>
              <property name="side">top</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Button" id="ButtonExtractText">
            <property name="command" type="command" cbtype="with_wid">set_operate</property>
            <property name="text" translatable="yes">Extract Text</property>
            <layout manager="pack">
              <property name="fill">x</property>
              <property name="ipadx">2</property>
              <property name="padx">4</property>
              <property name="pady">4</property>
              <property name="propagate">True</property>
              <property name="side">top</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Button" id="ButtonPDF2Images">
            <property name="command" type="command" cbtype="with_wid">set_operate</property>
            <property name="text" translatable="yes">PDF to Images</property>
            <layout manager="pack">
              <property name="fill">x</property>
              <property name="ipadx">2</property>
              <property name="padx">4</property>
              <property name="pady">4</property>
              <property name="propagate">True</property>
              <property name="side">top</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Button" id="ButtonImages2PDF">
            <property name="command" type="command" cbtype="with_wid">set_operate</property>
            <property name="text" translatable="yes">Images to PDF</property>
            <layout manager="pack">
              <property name="fill">x</property>
              <property name="ipadx">2</property>
              <property name="padx">4</property>
              <property name="pady">4</property>
              <property name="propagate">True</property>
              <property name="side">top</property>
            </layout>
          </object>
        </child>
      </object>
    </child>
  </object>
</interface>
